# Google Web Search API (required)
# Get an API key at https://console.cloud.google.com/apis/credentials
# Enable "Custom Search API" in your Google Cloud project
GOOGLE_AI_API_KEY=your_google_api_key

# Google Programmable Search Engine ID (required)
# Create one at https://programmablesearchengine.google.com/
# Configure it to "Search the entire web"
GOOGLE_CSE_ID=your_search_engine_id

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=000000000000
VITE_FIREBASE_APP_ID=your_firebase_app_id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX
